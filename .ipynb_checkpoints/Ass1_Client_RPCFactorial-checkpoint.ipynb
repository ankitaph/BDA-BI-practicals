    "\n",
    "n = int(input(\"Enter an integer value: \"))\n",
    "proxy = xmlrpc.client.ServerProxy('http://localhost:8000/')\n",
    "result = proxy.factorial(n)\n",
    "print(f\"The factorial of {n} is {result}\")\n"
